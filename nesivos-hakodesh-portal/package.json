{
  "name": "nesivos-hakodesh-portal",
  "description": "Nesivos Hakodesh App",
  "productName": "nesivos-hakodesh-portal",
  "author": {
    "name": "Maven Software Solutions"
  },
  "version": "1.1.0",
  "main-backup": "app.js",
  "main": "http://localhost:4200",
  "node-remote": "http://localhost:4200",
  "window": {
    "width": 960,
    "height": 600,
    "min_width": 700,
    "min_height": 500
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:nw": "concurrently \"ng serve --port=4200\" \"nw .\"",
    "mss-build:clean": "rimraf ./dist-ng ./dist",
    "mss-build:ng": "ng build --output-path ./dist-ng",
    "mss-build:nw": "ng build --concurrent --tasks win-x86,linux-x86,linux-x64,mac-x64 --mirror https://dl.nwjs.io/ .",
    "mss-build:win": "npm run mss-build:clean && npm run mss-build:ng && build --tasks win-x86 --mirror https://dl.nwjs.io/ .",
    "mss-build": "npm run mss-build:clean && npm run mss-build:ng && npm run mss-build:nw",
    "start:electron": "ng build --base-href ./ && electron .",
    "start:electron2": "ng build && electron .",
    "package-win": "ng build --base-href ./ && electron-packager . nesivos-hakodesh-portal --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Nesivos Hakodesh App\"",
    "package-win2": "ng build && electron-packager . nesivos-hakodesh-portal --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Nesivos Hakodesh App\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.17",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~12.2.17",
    "@angular/core": "~12.2.17",
    "@angular/forms": "~12.2.17",
    "@angular/localize": "^12.2.17",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/router": "~12.2.17",
    "@ckeditor/ckeditor5-angular": "^2.0.1",
    "@ckeditor/ckeditor5-basic-styles": "^28.0.0",
    "@ckeditor/ckeditor5-build-classic": "^28.0.0",
    "@ckeditor/ckeditor5-core": "^28.0.0",
    "@ckeditor/ckeditor5-dev-utils": "^25.2.2",
    "@ckeditor/ckeditor5-editor-classic": "^28.0.0",
    "@ckeditor/ckeditor5-essentials": "^28.0.0",
    "@ckeditor/ckeditor5-image": "^28.0.0",
    "@ckeditor/ckeditor5-paragraph": "^28.0.0",
    "@ckeditor/ckeditor5-theme-lark": "^28.0.0",
    "@ckeditor/ckeditor5-ui": "^28.0.0",
    "@hebcal/core": "^3.10.0",
    "@ng-bootstrap/ng-bootstrap": "^9.0.2",
    "@ng-select/ng-select": "^6.1.0",
    "@pdftron/pdfjs-express": "^7.3.2",
    "@sweetalert2/ngx-sweetalert2": "^9.0.0",
    "angular2-draggable": "^2.3.2",
    "bootstrap": "^4.6.0",
    "gematriya": "^2.0.0",
    "ng2-pdf-viewer": "^6.4.1",
    "ngx-bootstrap": "^6.2.0",
    "ngx-cookie": "^5.0.1",
    "ngx-extended-pdf-viewer": "^9.0.0-alpha.3",
    "ngx-infinite-scroll": "^10.0.1",
    "ngx-spinner": "^10.0.1",
    "ngx-toastr": "^13.2.1",
    "ngx-virtual-scroller": "^4.0.3",
    "rxjs": "~6.6.0",
    "sweetalert2": "^10.15.5",
    "tslib": "^2.0.0",
    "webpack": "^4.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.17",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^6.0.2",
    "electron-packager": "^15.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nw": "^0.52.2-sdk",
    "nwjs-builder-phoenix": "^1.15.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  },
  "build": {
    "nwVersion": "v0.44.4",
    "nwFlavor": "sdk",
    "files": [
      "**/*"
    ],
    "excludes": [
      "e2e/**/*",
      "src/**/*",
      ".editorconfig",
      ".gitignore",
      "angular.json",
      "browserslist",
      "karma.conf.js",
      "package-lock.json",
      "screenshot.png",
      "tsconfig.app.json",
      "tsconfig.json",
      "tsconfig.spec.json",
      "tslint.json",
      "release-builds/**/*"
    ],
    "strippedProperties": [
      "ManifestComments",
      "node-remote",
      "scripts",
      "devDependencies",
      "build"
    ],
    "overriddenProperties": {
      "main": "dist-ng/index.html",
      "node-remote": ""
    },
    "nsis": {
      "languages": [
        "English"
      ],
      "diffUpdaters": false,
      "hashCalculation": true
    }
  }
}